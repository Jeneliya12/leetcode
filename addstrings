public class AddStringsAsNumber
{
    public static void main(String[] args)
    {
        String str1 = "123";
        String str2 = "456";
        
        int num1 = Integer.parseInt(str1);
        int num2 = Integer.parseInt(str2);
        
        int sum = num1 + num2;
        
        System.out.println("The sum of two strings as a number is " + sum);
        
        
    }
}